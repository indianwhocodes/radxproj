#
# Recursive make - makes the subdirectory code
#

include $(RAP_MAKE_INC_DIR)/rap_make_macros

TARGETS =  $(GENERAL_TARGETS) $(DOC_TARGETS) $(INSTALL_TARGETS)

SUB_DIRS = \
	awc \
	colide \
	didss \
	dsserver \
	ingest \
	filters \
	fuzzy_logic \
	ingest \
	mdv_utils \
	ncwf \
	physics \
	procmap \
	rucApps \
	scripts \
	tdrp \
	titan \
	trec

$(TARGETS): FORCE
	$(MAKE) $(SUB_DIRS) TARGET=$@

$(SUB_DIRS): FORCE
	cd $@; $(MAKE) $(TARGET)

FORCE:







