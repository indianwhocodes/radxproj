TARGETS =  $(GENERAL_TARGETS) $(DOC_TARGETS) $(INSTALL_TARGETS)

SUB_DIRS = \
	ConvWx \
	CoSPAVx \
	didss \
	dsserver \
	filters \
	fuzzy_logic \
        hrrr \
	hydro \
	ingest \
	mdv_utils \
	niwot \
	procmap \
	rucApps \
	scripts \
	spdb_utils \
	tdrp \
	titan \
	wsddm

$(TARGETS): FORCE
